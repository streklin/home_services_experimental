<?xml version='1.0'?>

<robot name="marvin_bot" xmlns:xacro="http://www.ros.org/wiki/xacro">
	<xacro:include filename="$(find rover_platform)/urdf/marvin_bot.gazebo" />

	<link name="robot_footprint"></link>

	<joint name="robot_footprint_joint" type="fixed">
		<origin xyz="0 0 0" rpy="0 0 0" />
	    	<parent link="robot_footprint"/>
	   	<child link="chassis" />
	</joint>

	<link name="chassis">
		<pose>0 0 0 0 0 0</pose>
		<inertial>
			<mass value="10.0"/>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<inertia
          			ixx="0.1" ixy="0" ixz="0"
          			iyy="0.1" iyz="0"
          			izz="0.1"
      			/>
		</inertial>

		<collision name="rear_collision">
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<box size=".025 .18 .025" />
			</geometry>
		</collision>

		<visual name="rear_visual">
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<box size=".025 .18 .025" />
			</geometry>
		</visual>

		<collision name="rear_platform_collision">
			<origin xyz="0 0 0.025" rpy="0 0 0" />
			<geometry>
				<box size=".025 .1 .025" />
			</geometry>
		</collision>

		<visual name="rear_platform_visual">
			<origin xyz="0 0 0.025" rpy="0 0 0" />
			<geometry>
				<box size="0.025 .1 .025" />
			</geometry>
		</visual>

		<collision name="front_platform_collision">
			<origin xyz="0.05 0 0" rpy="0 0 0" />
			<geometry>
				<box size=".1 .05 .025" />
			</geometry>
		</collision>

		<visual name="front_platform_visual">
			<origin xyz="0.05 0 0" rpy="0 0 0" />
			<geometry>
				<box size=".1 .05 .025" />
			</geometry>
		</visual>
	</link>

	<link name="left_wheel">
		<pose>0 0 0 0 1.5707 1.5707</pose>

		<inertial>
			<mass value="10.0"/>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<inertia
          			ixx="0.1" ixy="0" ixz="0"
          			iyy="0.1" iyz="0"
          			izz="0.1"
      			/>
		</inertial>

		<collision>
			<origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
			<geometry>
				<cylinder radius="0.025" length="0.025" />
			</geometry>
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
			<geometry>
				<cylinder radius="0.025" length="0.025" />
			</geometry>
		</visual>
	</link>

	<joint name="left_wheel_hinge" type="continuous">
		<origin xyz="0 0.09 0" rpy="0 0 0"/>
		<child link="left_wheel" />
		<parent link="chassis"/>
		<axis xyz="0 1 0" rpy="0 0 0"/>
		<limit effort="10000" velocity="1000"/>
		<joint_properties damping="1.0" friction="1.0"/>
	</joint>

	<link name="right_wheel">
		<pose>0 0 0 0 1.5707 1.5707</pose>

		<inertial>
			<mass value="10.0"/>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<inertia
          			ixx="0.1" ixy="0" ixz="0"
          			iyy="0.1" iyz="0"
          			izz="0.1"
      			/>
		</inertial>

		<collision>
			<origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
			<geometry>
				<cylinder radius="0.025" length="0.025" />
			</geometry>
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
			<geometry>
				<cylinder radius="0.025" length="0.025" />
			</geometry>
		</visual>
	</link>

	<joint name="right_wheel_hinge" type="continuous">
		<origin xyz="0 -0.09 0" rpy="0 0 0"/>
		<child link="right_wheel" />
		<parent link="chassis"/>
		<axis xyz="0 1 0" rpy="0 0 0"/>
		<limit effort="10000" velocity="1000"/>
		<joint_properties damping="1.0" friction="1.0"/>
	</joint>

	<link name="counterbalance">
		<pose>0 0 0 0 0 0</pose>
		<inertial>
			<mass value="10.0"/>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<inertia
          			ixx="0.1" ixy="0" ixz="0"
          			iyy="0.1" iyz="0"
          			izz="0.1"
      			/>
		</inertial>

		<collision>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<box size=".01 .01 .01" />
			</geometry>
			<surface>
				<friction>
				  <ode>
				    <mu>0</mu>
				    <mu2>0</mu2>
				    <slip1>1.0</slip1>
				    <slip2>1.0</slip2>
				  </ode>
				</friction>
			 </surface>
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<sphere radius="0.028" />
			</geometry>
		</visual>
	</link>

	<joint name="counterbalance_joint" type="fixed">
		<origin xyz="0.07 0 0" rpy="0 0 0" />
	    	<parent link="chassis"/>
	   	<child link="counterbalance" />
	</joint>


	<link name="hokuyo">
	    <pose>0 0 0 0 0 0</pose>
	    <inertial>
	       <mass value="0.1"/>
	       <origin xyz="0 0 0" rpy="0 0 0"/>
	       <inertia
		 ixx="1e-6" ixy="0" ixz="0"
		 iyy="1e-6" iyz="0"
		 izz="1e-6"
		/>
	    </inertial>

	    <collision>
	       <origin xyz="0 0 0" rpy="0 0 0"/>
	       <geometry>
		  <box size="0.1 0.1 0.1" />
	       </geometry>
	    </collision>

	    <visual>
	       <origin xyz="0 0 0" rpy="0 0 0"/>
	       <geometry>
		  <mesh filename="package://rover_platform/meshes/hokuyo.dae"/>
	       </geometry>
	    </visual>
	  </link>

	  <joint type="fixed" name="hokuyo_joint">
	    <origin xyz="0.0 0 0.05" rpy="0 -0.2 0"/>
	    <child link="hokuyo"/>
	    <parent link="chassis"/>
	    <axis xyz="0 1 0" rpy="0 0 0"/>
	    <limit effort="10000" velocity="1000"/>
	    <joint_properties damping="1.0" friction="1.0"/>
	  </joint>


	  <link name="camera">
	    <pose>0 0 0 0 0 0</pose>
	    <inertial>
	       <mass value="0.1"/>
	       <origin xyz="0 0 0" rpy="0 0 0"/>
	       <inertia
		 ixx="1e-6" ixy="0" ixz="0"
		 iyy="1e-6" iyz="0"
		 izz="1e-6"
		/>
	    </inertial>

	    <collision>
	       <origin xyz="0 0 0" rpy="0 0 0"/>
	       <geometry>
		  <box size="0.01 0.01 0.01" />
	       </geometry>
	    </collision>

	    <visual>
	       <origin xyz="0 0 0" rpy="0 0 0"/>
	       <geometry>
		  <box size="0.01 0.01 0.01" />
	       </geometry>
	    </visual>

	  </link>

   <link name="camera_frame">
		 <pose>0 0 0 0 0 0</pose>
	 </link>

	  <joint type="fixed" name="camera_joint">
	    <origin xyz="0.07 0 0.035" rpy="0 -0.2 0"/>
	    <child link="camera"/>
	    <parent link="chassis"/>
	    <axis xyz="0 1 0" rpy="0 0 0"/>
	    <limit effort="10000" velocity="1000"/>
	    <joint_properties damping="1.0" friction="1.0"/>
	  </joint>

		<joint type="fixed" name="camera_frame_joint">
			<origin xyz="0.07 0 0.035" rpy="-1.57 0 -1.57"/>
			<child link="camera_frame"/>
			<parent link="camera"/>
			<axis xyz="1 0 0" rpy="0 0 0"/>
			<limit effort="10000" velocity="1000"/>
			<joint_properties damping="1.0" friction="1.0"/>
		</joint>

</robot>
